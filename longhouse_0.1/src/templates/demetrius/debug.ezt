[is dbg "off"]
 [if-any demetrius_perms.view_debug]
   <div class="debug">
    - <a href="[debug_uri]">Reload w/ debug info</a>
   </div>
 [end]
[else]
   <div class="debug">EZT Data
     <table class="ifOpened" cellpadding=2 cellspacing=2 border=0  style="padding-left: 1em">
      [for debug]
       <tr><th>[debug.key]</th><td>[debug.val]</td></tr>
      [end]
     </table>
   </div>
[end]

